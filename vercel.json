{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tickets/[transactionReference].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/tickets/(.*)",
      "dest": "/api/tickets/[transactionReference].js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
